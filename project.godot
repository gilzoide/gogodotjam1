[application]

config/name="Go Godot Ball Growth Game"
run/main_scene="res://gameplay/gameplay.tscn"
config/icon="res://icon.png"

[autoload]

GestureInputManager="*res://addons/Godot-Touch-Input-Manager/InputManager.gd"

[display]

window/size/width=480
window/size/height=720
window/handheld/orientation="sensor_portrait"

[input_devices]

pointing/emulate_mouse_from_touch=false

[layer_names]

2d_physics/layer_1="General"
2d_physics/layer_2="Player"
2d_physics/layer_3="Mob"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
